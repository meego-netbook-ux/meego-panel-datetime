msgstr "(غدا) %b %-d"

#: ../src/mnp-utils.c:538
msgid "%b %-d (local)"
msgstr "(محلّي) %b %-d"

#: ../src/mnp-utils.c:540
msgid "%b %-d"
msgstr "%b %-d"

