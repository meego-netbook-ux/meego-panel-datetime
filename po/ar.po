msgstr "(غدا) %b %-d"

#: ../src/mnp-utils.c:454
msgid "%b %-d (local)"
msgstr "(محلّي) %b %-d"

#: ../src/mnp-utils.c:456
msgid "%b %-d"
msgstr "%b %-d"

