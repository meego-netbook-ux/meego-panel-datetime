msgstr "%b %-d (Mañá)"

#: ../src/mnp-utils.c:454
msgid "%b %-d (local)"
msgstr "%b %-d (local)"

#: ../src/mnp-utils.c:456
msgid "%b %-d"
msgstr "%b %-d"

