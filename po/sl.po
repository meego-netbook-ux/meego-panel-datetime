msgstr "%b %-d (jutri)"

#: ../src/mnp-utils.c:538
msgid "%b %-d (local)"
msgstr "%b %-d (krajevno)"

#: ../src/mnp-utils.c:540
msgid "%b %-d"
msgstr "%b %-d"

