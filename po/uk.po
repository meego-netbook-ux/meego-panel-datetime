msgstr "%b %-d (Завтра)"

#: ../src/mnp-utils.c:454
msgid "%b %-d (local)"
msgstr "%b %-d (місцевий час)"

#: ../src/mnp-utils.c:456
msgid "%b %-d"
msgstr "%b %-d"

